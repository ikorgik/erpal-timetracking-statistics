{
  "manifest_version": 2,
  "name": "ERPAL Timetracking Statistics",
  "description": "This extension shows an ERPAL Timetracking in details.",
  "version": "1.0.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}